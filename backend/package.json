{
  "name": "jobfinder-backend",
  "version": "1.0.0",
  "description": "Backend api for jobfinder, Use application with JWT authentication and LoopBack 4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "examples/todo-jwt"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean *example-todo-jwt*.tgz dist *.tsbuildinfo package",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "verify": "npm pack && tar xf loopback-todo*.tgz && tree package && npm run clean",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node ."
  },
  "dependencies": {
    "@loopback/authentication": "^7.0.1",
    "@loopback/authentication-jwt": "^0.7.1",
    "@loopback/boot": "^3.0.1",
    "@loopback/build": "^6.2.4",
    "@loopback/core": "^2.10.1",
    "@loopback/eslint-config": "^10.0.0",
    "@loopback/http-caching-proxy": "^2.1.15",
    "@loopback/repository": "^3.0.1",
    "@loopback/rest": "^7.0.1",
    "@loopback/rest-explorer": "^3.0.1",
    "@loopback/security": "^0.3.1",
    "@loopback/service-proxy": "^3.0.1",
    "@loopback/testlab": "^3.2.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash": "^4.14.161",
    "@types/node": "^10.17.35",
    "@types/nodemailer": "^6.4.0",
    "@types/randomstring": "^1.1.6",
    "@types/uuid": "^8.3.0",
    "bcryptjs": "^2.4.3",
    "eslint": "^7.9.0",
    "fetch": "^1.1.0",
    "isemail": "^3.2.0",
    "lodash": "^4.17.20",
    "loopback": "^3.28.0",
    "loopback-connector-mongodb": "^5.4.0",
    "loopback-connector-rest": "^3.7.0",
    "nodemailer": "^6.4.16",
    "nodemon": "^2.0.7",
    "paypal-adaptive": "^0.4.2",
    "randomstring": "^1.1.5",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "tslib": "^2.0.1",
    "typescript": "~4.0.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.4",
    "@loopback/eslint-config": "^10.0.0",
    "@loopback/http-caching-proxy": "^2.1.15",
    "@loopback/testlab": "^3.2.6",
    "@types/lodash": "^4.14.161",
    "@types/node": "^10.17.35",
    "eslint": "^7.9.0",
    "lodash": "^4.17.20",
    "typescript": "~4.0.2"
  },
  "keywords": [
    "loopback",
    "LoopBack",
    "example",
    "tutorial",
    "CRUD",
    "models",
    "todo",
    "jwt",
    "authentication"
  ]
}
